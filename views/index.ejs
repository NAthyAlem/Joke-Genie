<!DOCTYPE html>
<html>
  <head>
    <title>Jokkee</title>
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Gloria+Hallelujah&family=Titan+One&display=swap"
      rel="stylesheet"
    />
    <link rel="stylesheet" type="text/css" href="styles/main.css" />
  </head>

  <body>
    <h1>Randomly Generates Jokes</h1>
    <div class="card">
      <p class="content"><%= contentt %></p>
      <p class="delivery"><%= deliveryy %></p>
    </div>
    <p class="user"><%= categoryy %></p>
    <script>
      document.addEventListener("DOMContentLoaded", function () {
        //wait for the DOM content to be loaded
        setTimeout(function () {
          document.querySelector(".delivery").classList.add("visible");
        }, 5000);
      });
    </script>
  </body>
</html>
